{
    "name": "grajewsky/object-mapper",
    "description": "PHP Object mapper",
    "type": "library",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "pestphp/pest": "^1.0",
        "phpstan/phpstan": "^1.8",
        "symfony/var-dumper": "^6.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Grajewsky\\ObjectMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grajewsky\\ObjectMapper\\Testing\\": "testing/"
        }
    },
    "authors": [
        {
            "name": "≈Åukasz Grajewski",
            "email": "grajewski.lukasz@wp.pl"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
